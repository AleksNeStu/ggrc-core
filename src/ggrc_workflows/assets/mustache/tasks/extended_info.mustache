{{#instance}}
{{!div class="extended"}}
  <div class="row-fluid">
    <div class="span12">
      <a class="main-title {{class.category}} oneline" href="{{viewLink}}">
        {{title}}
      </a>
    </div>
  </div>

  <div class="row-fluid">
    <div class="span12">
      <div class="rtf">
        <p>
          {{{firstnonempty description '<span class="empty-message">None</span>'}}}
        </p>
      </div>

      {{^if_null url}}
        <a class="url oneline" href="{{schemed_url url}}" target="_blank">
          {{url}}
        </a>
      {{/if_null}}
      {{#if_null url}}
        <span class="no-url">No URL</span>
      {{/if_null}}

    </div>
  </div>

  <div class="row-fluid">
    <div class="span12">
      <div class="code">
        Code: {{slug}}
      </div>
    </div>
  </div>

  <div class="links">
    <div class="row-fluid">
      <div class="span6">
        <a class="secondary" href="{{viewLink}}">
          View {{class.title_singular}}
        </a>
      </div>
    </div>
  </div>

{{/instance}}
